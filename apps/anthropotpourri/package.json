{
	"name": "anthropotpourri",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --open --port 5174",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pnpm eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"lint:fix": "pnpm eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@apps-by-alice/lint": "workspace:*",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.57.0",
		"postcss": "^8.4.35",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte-check": "^3.6.4",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^4.5.2"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.14",
		"@vercel/analytics": "^1.2.2",
