{
	"name": "tempo-immaterial",
	"version": "0.4.1",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "pnpm eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"lint:fix": "pnpm eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@apps-by-alice/lint": "workspace:*",
		"@playwright/test": "^1.44.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-vercel": "^5.3.2",
		"@sveltejs/kit": "^2.5.17",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@vercel/analytics": "^1.3.1",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"mdsvex": "^0.11.2",
		"postcss": "^8.4.38",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"sass": "^1.77.6",
		"svelte-check": "^3.8.1",
		"svelte-preprocess": "^5.1.4",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.5.2",
		"vite": "^5.3.1",
		"vite-imagetools": "^5.1.2",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.15",
		"@sveltejs/enhanced-img": "^0.1.9",
		"@vercel/speed-insights": "^1.0.12",
		"gsap": "^3.12.5",
		"highlight.js": "^11.9.0",
		"motion": "^10.18.0",
		"sharp": "^0.33.4",
		"svelte": "^4.2.18",
		"svelte-highlight": "^7.6.1",
		"tslib": "^2.6.3",
		"web-vitals": "^3.5.2"
	},
	"packageManager": "pnpm@8.7.4"
}