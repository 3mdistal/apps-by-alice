{
	"name": "tempo-immaterial",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "pnpm eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"lint:fix": "pnpm eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@apps-by-alice/lint": "workspace:*",
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.24.1",
		"@vercel/analytics": "^1.0.2",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.54.0",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"sass": "^1.66.1",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-imagetools": "^5.0.8",
		"vitest": "^0.34.4"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.13",
		"@vercel/speed-insights": "^1.0.10",
		"gsap": "^3.12.2",
		"highlight.js": "^11.8.0",
		"mermaid": "^10.4.0",
		"motion": "^10.16.2",
		"svelte": "^4.2.0",
		"svelte-highlight": "^7.3.0",
		"tslib": "^2.6.2",
		"web-vitals": "^3.4.0"
	},
	"packageManager": "pnpm@8.7.4"
}
