{
	"name": "tempo-immaterial",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "pnpm eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"lint:fix": "pnpm eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@apps-by-alice/lint": "workspace:*",
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.30.4",
		"@vercel/analytics": "^1.2.2",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.57.0",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.35",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"sass": "^1.71.1",
		"svelte-check": "^3.6.4",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^4.5.2",
		"vite-imagetools": "^5.1.2",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.14",
		"@vercel/speed-insights": "^1.0.10",
		"gsap": "^3.12.5",
		"highlight.js": "^11.9.0",
		"mermaid": "^10.8.0",
		"motion": "^10.17.0",
		"svelte": "^4.2.12",
		"svelte-highlight": "^7.6.0",
		"tslib": "^2.6.2",
		"web-vitals": "^3.5.2"
	},
	"packageManager": "pnpm@8.7.4"
}
